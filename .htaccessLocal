RewriteEngine On

RewriteCond %{REQUEST_URI} \.(css|js|png|jpg|jpeg|gif|svg|webp|ico|woff|woff2|ttf|mp3|wav|ogg)$ [NC]
RewriteRule ^ - [L]

RewriteCond %{REQUEST_URI} /arriola_music/index\.php$ [NC]
RewriteRule ^ - [L]

RewriteCond %{REQUEST_URI} ^/arriola_music/frontend/admin/(login|configuracion|dashboard|galeria|tour|usuarios)\.php$ [NC]
RewriteRule ^ - [L]

RewriteCond %{REQUEST_URI} ^/arriola_music/$
RewriteRule ^ - [L]

RewriteRule ^.*$ /arriola_music/index.php [L,R=302]